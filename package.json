{
  "name": "cto-website",
  "description": "Website for the GSA Office of the Chief Technology Officer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/cto-website.git"
  },
  "author": "GSA",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GSA/cto-website/issues"
  },
  "homepage": "https://github.com/GSA/cto-website#readme",
  "scripts": {
    "assets": "gulp copy-assets",
    "test:serve": "bundle exec jekyll build && http-server -s -p 4567 _site",
    "test:a11y:live": "pa11y-ci --sitemap https://tech.gsa.gov/sitemap.xml",
    "test:a11y:local": "pa11y-ci --sitemap http://localhost:4567/sitemap.xml --sitemap-find https://tech.gsa.gov --sitemap-replace http://localhost:4567",
    "test:a11y:ci": "start-server-and-test test:serve 4567 test:a11y:local"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "http-server": "^0.12.1",
    "lodash": "^4.17.15",
    "pa11y-ci": "^2.3.0",
    "start-server-and-test": "^1.11.0"
  },
  "dependencies": {
    "extend": "^3.0.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lunr": "^2.3.6",
    "uswds": "^1.6.12",
    "waypoints": "^4.0.1"
  }
}
